{
  "name": "react-jw-player",
  "version": "2.0.0-alpha.1",
  "description": "A React component for launching JW Player instances on the client.",
  "main": "dist/react-jw-player.js",
  "scripts": {
    "build:base": "babel src/base --out-dir base --ignore '*.test.js'",
    "precommit": "lint-staged",
    "test": "eslint 'src/**/*.js' && flow && npm run test:unit",
    "test:unit": "babel-tape-runner 'src/**/*.js' | tap-spec"
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/micnews/react-jw-player.git"
  },
  "keywords": [
    "react",
    "jwplayer",
    "jw-player",
    "video",
    "autoplay",
    "events"
  ],
  "author": "Mic Network, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/micnews/react-jw-player/issues"
  },
  "homepage": "https://github.com/micnews/react-jw-player#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-tape-runner": "^2.0.1",
    "enzyme": "^2.9.1",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-mic": "^1.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.39.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.10.3",
    "flow": "^0.2.3",
    "flow-bin": "^0.57.2",
    "husky": "^0.14.3",
    "jsdom": "^11.3.0",
    "jsdom-global": "^3.0.2",
    "lint-staged": "^4.2.3",
    "react-dom": "^15.6.2",
    "react-test-renderer": "^15.6.2",
    "tap-spec": "^4.1.1",
    "tape": "^4.8.0"
  },
  "dependencies": {
    "react": "^15.6.2"
  }
}
